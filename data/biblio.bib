	scopus = {2-s2.0-85056373026},
	shorttitle = {{CASCA}},
	title = {{CASCA}: {A} {Design} {Automation} {Approach} for {Designing} {Hardware} {Countermeasures} {Against} {Side}-{Channel} {Attacks}},
